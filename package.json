{
  "name": "appfolio-react-template",
  "version": "0.1.0",
  "description": "Appfolio template for ES2015, React, Webpack, and Babel.",
  "author": "Appfolio, Inc.",
  "license": "MIT",
  "main": "dev_server.js",
  "scripts": {
    "dist": "webpack -p",
    "lint": "eslint src; exit 0",
    "start": "webpack-dashboard -- node dev_server.js",
    "test": "nyc --reporter text --reporter lcov mocha --recursive --timeout 10000 --compilers js:babel-register --require ignore-styles"
  },
  "dependencies": {
    "babel-polyfill": "~6.13.0",
    "react": "~15.3.0",
    "react-dom": "~15.3.0"
  },
  "devDependencies": {
    "assert": "~1.4.1",
    "autoprefixer": "~6.3.7",
    "babel-core": "~6.13.2",
    "babel-loader": "~6.2.4",
    "babel-plugin-transform-object-assign": "~6.8.0",
    "babel-preset-es2015": "~6.13.0",
    "babel-preset-react": "~6.11.1",
    "babel-preset-stage-1": "~6.13.0",
    "babel-preset-stage-2": "~6.13.0",
    "css-loader": "~0.23.1",
    "eslint-config-appfolio-react": "~0.2.0",
    "express": "~4.14.0",
    "ignore-styles": "~4.0.0",
    "lodash": "~4.6.1",
    "mocha": "~2.5.3",
    "nightmare": "~2.5.2",
    "node-sass": "~3.8.0",
    "nyc": "~6.6.1",
    "postcss": "~5.1.0",
    "postcss-loader": "~0.9.1",
    "sass-loader": "~4.0.0",
    "style-loader": "~0.13.0",
    "webpack": "~2.1.0-beta.13",
    "webpack-dashboard": "~0.1.1",
    "webpack-dev-middleware": "~1.6.1",
    "webpack-hot-middleware": "~2.12.1",
    "webpack-visualizer-plugin": "~0.1.5"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dev_server.js",
      "webpack.*config.js"
    ]
  }
}
