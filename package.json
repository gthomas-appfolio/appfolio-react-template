{
  "name": "appfolio-js-template",
  "version": "0.2.0",
  "description": "Appfolio template for ES2015, Webpack, Babel, and testing tools.",
  "author": "Appfolio, Inc.",
  "license": "MIT",
  "main": "dev_server.js",
  "scripts": {
    "dist": "webpack -p",
    "lint": "eslint src; exit 0",
    "start": "node dev_server.js",
    "test": "nyc --reporter text --reporter lcov mocha --recursive --timeout 10000 --compilers js:babel-register --require ignore-styles"
  },
  "dependencies": {
    "babel-polyfill": "^6.9.1"
  },
  "devDependencies": {
    "assert": "^1.4.1",
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "~6.9.0",
    "babel-preset-stage-1": "^6.5.0",
    "babel-preset-stage-2": "^6.11.0",
    "css-loader": "~0.23.1",
    "eslint-config-appfolio-base": "^0.1.0",
    "express": "^4.14.0",
    "ignore-styles": "^4.0.0",
    "lodash": "^4.6.1",
    "mocha": "~2.5.3",
    "nightmare": "^2.5.2",
    "node-sass": "~3.8.0",
    "nyc": "^7.0.0",
    "postcss": "^5.1.0",
    "postcss-loader": "~0.9.1",
    "sass-loader": "^4.0.0",
    "style-loader": "~0.13.0",
    "webpack": "~2.1.0-beta.13",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.1",
    "webpack-visualizer-plugin": "^0.1.5"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "dev_server.js",
      "webpack.*config.js"
    ]
  }
}
